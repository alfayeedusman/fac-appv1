# Copy this file to .env and fill in the values.
#
# IMPORTANT:
# - Never commit your real .env file.
# - Variables starting with VITE_ are exposed to the frontend build.

# -----------------------------
# Database (Neon Postgres)
# -----------------------------
NEON_DATABASE_URL=
DATABASE_URL=

# -----------------------------
# Database (MySQL - Docker Compose)
# -----------------------------
MYSQL_PASSWORD=
MYSQL_ROOT_PASSWORD=

# -----------------------------
# Firebase (Frontend)
# -----------------------------
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=
VITE_FIREBASE_FCM_KEY=

# -----------------------------
# Firebase Admin (Backend - Optional)
# Used only if you enable server-side push delivery.
# -----------------------------
FIREBASE_PROJECT_ID=
FIREBASE_PRIVATE_KEY_ID=
FIREBASE_PRIVATE_KEY=
FIREBASE_CLIENT_EMAIL=
FIREBASE_CLIENT_ID=

# -----------------------------
# Maps (Frontend)
# -----------------------------
VITE_MAPBOX_TOKEN=

# -----------------------------
# Payments (Xendit)
# -----------------------------
XENDIT_SECRET_KEY=
XENDIT_PUBLIC_KEY=
XENDIT_WEBHOOK_TOKEN=

# -----------------------------
# Real-time (Pusher)
# -----------------------------
PUSHER_APP_ID=
PUSHER_KEY=
PUSHER_SECRET=
PUSHER_CLUSTER=
VITE_PUSHER_KEY=
VITE_PUSHER_CLUSTER=

# -----------------------------
# App URLs
# -----------------------------
FRONTEND_URL=

# -----------------------------
# Email (Backend - Optional)
# -----------------------------
EMAIL_USER=
EMAIL_APP_PASSWORD=
